{
  "name": "solid-petstore",
  "type": "module",
  "version": "1.0.0",
  "description": "A skeleton solidjs app for the petstore api.",
  "keywords": [
    "chubbyts",
    "skleton",
    "solid"
  ],
  "author": "Dominik Zogg",
  "license": "MIT",
  "repository": "chubbyts/solid-petstore",
  "scripts": {
    "build": "tsc && vite build",
    "cs-fix": "prettier --write src tests *.js *.ts",
    "cs": "prettier --check src tests *.js *.ts",
    "develop": "vite",
    "lint-fix": "eslint src tests *.js *.ts --fix",
    "lint": "eslint src tests",
    "start": "pnpm i && pnpm cs-fix && pnpm develop",
    "test": "vitest"
  },
  "dependencies": {
    "@chubbyts/chubbyts-throwable-to-error": "^1.3.2",
    "@solidjs/router": "^0.15.3",
    "cross-fetch": "^4.1.0",
    "date-fns": "^4.1.0",
    "qs": "^6.14.0",
    "solid-js": "^1.9.4",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@chubbyts/chubbyts-function-mock": "^1.5.2",
    "@eslint/js": "^9.19.0",
    "@prettier/sync": "^0.5.2",
    "@solidjs/testing-library": "^0.8.10",
    "@testing-library/user-event": "^14.6.1",
    "@types/js-beautify": "^1.14.3",
    "@types/node": "^22.13.0",
    "@types/qs": "^6.9.18",
    "@vitest/coverage-v8": "^3.0.5",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.19.0",
    "eslint-import-resolver-typescript": "^3.7.0",
    "eslint-plugin-functional": "^8.0.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-tailwindcss": "^3.18.0",
    "eslint-plugin-unused-imports": "^4.1.4",
    "js-beautify": "^1.15.1",
    "jsdom": "^26.0.0",
    "postcss": "^8.5.1",
    "prettier": "^3.4.2",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.23.0",
    "vite": "^6.0.11",
    "vite-plugin-solid": "2.11.1",
    "vitest": "^3.0.5"
  },
  "engines": {
    "node": ">=18"
  },
  "prettier": {
    "printWidth": 120,
    "tabWidth": 2,
    "singleQuote": true,
    "trailingComma": "all"
  },
  "packageManager": "pnpm@10.2.0+sha512.0d27364e0139c6aadeed65ada153135e0ca96c8da42123bd50047f961339dc7a758fc2e944b428f52be570d1bd3372455c1c65fa2e7aa0bfbf931190f9552001",
