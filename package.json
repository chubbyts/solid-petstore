{
  "name": "solid-petstore",
  "type": "module",
  "description": "A skeleton solidjs app for the petstore api.",
  "keywords": [
    "chubbyts",
    "skleton",
    "solid"
  ],
  "author": "Dominik Zogg",
  "license": "MIT",
  "repository": "chubbyts/solid-petstore",
  "scripts": {
    "build": "tsc && vite build",
    "cs-fix": "prettier --write src tests *.js *.ts",
    "cs": "prettier --check src tests *.js *.ts",
    "develop": "vite",
    "lint-fix": "eslint src tests *.js *.ts --fix",
    "lint": "eslint src tests",
    "start": "pnpm i && pnpm cs-fix && pnpm develop",
    "test": "vitest"
  },
  "dependencies": {
    "@chubbyts/chubbyts-throwable-to-error": "^2.0.2",
    "@solidjs/router": "^0.15.4",
    "@tanstack/solid-query": "^5.90.19",
    "cross-fetch": "^4.1.0",
    "date-fns": "^4.1.0",
    "nock": "^14.0.10",
    "qs": "^6.14.1",
    "solid-js": "^1.9.10",
    "zod": "^4.3.4"
  },
  "devDependencies": {
    "@chubbyts/chubbyts-function-mock": "^2.0.2",
    "@eslint/js": "^9.39.2",
    "@prettier/sync": "^0.6.1",
    "@solidjs/testing-library": "^0.8.10",
    "@tailwindcss/postcss": "^4.1.18",
    "@testing-library/user-event": "^14.6.1",
    "@types/js-beautify": "^1.14.3",
    "@types/node": "^25.0.3",
    "@types/qs": "^6.14.0",
    "@vitest/coverage-v8": "^4.0.16",
    "eslint": "^9.39.2",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-functional": "^9.0.2",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-solid": "^0.14.5",
    "eslint-plugin-unused-imports": "^4.3.0",
    "js-beautify": "^1.15.4",
    "jsdom": "^27.4.0",
    "postcss": "^8.5.6",
    "prettier": "^3.7.4",
    "prettier-plugin-tailwindcss": "^0.7.2",
    "tailwindcss": "^4.1.18",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.51.0",
    "vite": "^7.3.0",
    "vite-plugin-solid": "2.11.10",
    "vitest": "^4.0.16"
  },
  "engines": {
    "node": ">=20"
  },
  "prettier": {
    "printWidth": 120,
    "tabWidth": 2,
    "singleQuote": true,
    "trailingComma": "all",
    "plugins": [
      "prettier-plugin-tailwindcss"
    ]
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "@tailwindcss/oxide",
      "esbuild",
      "unrs-resolver"
    ]
  },
  "packageManager": "pnpm@10.27.0+sha512.72d699da16b1179c14ba9e64dc71c9a40988cbdc65c264cb0e489db7de917f20dcf4d64d8723625f2969ba52d4b7e2a1170682d9ac2a5dcaeaab732b7e16f04a"
}
