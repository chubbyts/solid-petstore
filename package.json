{
  "name": "solid-petstore",
  "type": "module",
  "description": "A skeleton solidjs app for the petstore api.",
  "keywords": [
    "chubbyts",
    "skleton",
    "solid"
  ],
  "author": "Dominik Zogg",
  "license": "MIT",
  "repository": "chubbyts/solid-petstore",
  "scripts": {
    "build": "tsc && vite build",
    "cs-fix": "prettier --write src tests *.js *.ts",
    "cs": "prettier --check src tests *.js *.ts",
    "develop": "vite",
    "lint-fix": "eslint src tests *.js *.ts --fix",
    "lint": "eslint src tests",
    "start": "pnpm i && pnpm cs-fix && pnpm develop",
    "test": "vitest"
  },
  "dependencies": {
    "@chubbyts/chubbyts-throwable-to-error": "^2.0.2",
    "@solidjs/router": "^0.15.3",
    "@tanstack/solid-query": "^5.90.3",
    "cross-fetch": "^4.1.0",
    "date-fns": "^4.1.0",
    "nock": "^14.0.10",
    "qs": "^6.14.0",
    "solid-js": "^1.9.9",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@chubbyts/chubbyts-function-mock": "^2.0.2",
    "@eslint/js": "^9.37.0",
    "@prettier/sync": "^0.6.1",
    "@solidjs/testing-library": "^0.8.10",
    "@tailwindcss/postcss": "^4.1.14",
    "@testing-library/user-event": "^14.6.1",
    "@types/js-beautify": "^1.14.3",
    "@types/node": "^24.7.1",
    "@types/qs": "^6.14.0",
    "@vitest/coverage-v8": "^3.2.4",
    "eslint": "^9.37.0",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-functional": "^9.0.2",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-solid": "^0.14.5",
    "eslint-plugin-unused-imports": "^4.2.0",
    "js-beautify": "^1.15.4",
    "jsdom": "^27.0.0",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "tailwindcss": "^4.1.14",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.46.0",
    "vite": "^7.1.9",
    "vite-plugin-solid": "2.11.9",
    "vitest": "^3.2.4"
  },
  "engines": {
    "node": ">=20"
  },
  "prettier": {
    "printWidth": 120,
    "tabWidth": 2,
    "singleQuote": true,
    "trailingComma": "all",
    "plugins": [
      "prettier-plugin-tailwindcss"
    ]
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "@tailwindcss/oxide",
      "esbuild",
      "unrs-resolver"
    ]
  },
  "packageManager": "pnpm@10.12.1+sha512.f0dda8580f0ee9481c5c79a1d927b9164f2c478e90992ad268bbb2465a736984391d6333d2c327913578b2804af33474ca554ba29c04a8b13060a717675ae3ac"
}
