{
  "name": "solid-petstore",
  "type": "module",
  "description": "A skeleton solidjs app for the petstore api.",
  "keywords": [
    "chubbyts",
    "skleton",
    "solid"
  ],
  "author": "Dominik Zogg",
  "license": "MIT",
  "repository": "chubbyts/solid-petstore",
  "scripts": {
    "build": "tsc && vite build",
    "cs-fix": "prettier --write src tests *.js *.ts",
    "cs": "prettier --check src tests *.js *.ts",
    "develop": "vite",
    "lint-fix": "eslint src tests *.js *.ts --fix",
    "lint": "eslint src tests",
    "start": "pnpm i && pnpm cs-fix && pnpm develop",
    "test": "vitest"
  },
  "dependencies": {
    "@chubbyts/chubbyts-throwable-to-error": "^2.0.2",
    "@solidjs/router": "^0.15.4",
    "@tanstack/solid-query": "^5.90.15",
    "cross-fetch": "^4.1.0",
    "date-fns": "^4.1.0",
    "nock": "^14.0.10",
    "qs": "^6.14.0",
    "solid-js": "^1.9.10",
    "zod": "^4.1.13"
  },
  "devDependencies": {
    "@chubbyts/chubbyts-function-mock": "^2.0.2",
    "@eslint/js": "^9.39.1",
    "@prettier/sync": "^0.6.1",
    "@solidjs/testing-library": "^0.8.10",
    "@tailwindcss/postcss": "^4.1.18",
    "@testing-library/user-event": "^14.6.1",
    "@types/js-beautify": "^1.14.3",
    "@types/node": "^25.0.1",
    "@types/qs": "^6.14.0",
    "@vitest/coverage-v8": "^4.0.15",
    "eslint": "^9.39.1",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-functional": "^9.0.2",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-solid": "^0.14.5",
    "eslint-plugin-unused-imports": "^4.3.0",
    "js-beautify": "^1.15.4",
    "jsdom": "^27.3.0",
    "postcss": "^8.5.6",
    "prettier": "^3.7.4",
    "prettier-plugin-tailwindcss": "^0.7.2",
    "tailwindcss": "^4.1.18",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.49.0",
    "vite": "^7.2.7",
    "vite-plugin-solid": "2.11.10",
    "vitest": "^4.0.15"
  },
  "engines": {
    "node": ">=20"
  },
  "prettier": {
    "printWidth": 120,
    "tabWidth": 2,
    "singleQuote": true,
    "trailingComma": "all",
    "plugins": [
      "prettier-plugin-tailwindcss"
    ]
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "@tailwindcss/oxide",
      "esbuild",
      "unrs-resolver"
    ]
  },
  "packageManager": "pnpm@10.25.0+sha512.5e82639027af37cf832061bcc6d639c219634488e0f2baebe785028a793de7b525ffcd3f7ff574f5e9860654e098fe852ba8ac5dd5cefe1767d23a020a92f501"
}
